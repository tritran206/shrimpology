<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tritran.shrimpology">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ShrimpsHome" />
        <activity android:name=".CaridinaActivity" />
        <activity android:name=".NeocaridinaActvity" />
        <activity android:name=".AddAquarium" />
        <activity android:name=".ShrimpDetails" />
        <activity
            android:name=".AquariumsHome"
            android:label="@string/title_activity_aquarium_home"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>